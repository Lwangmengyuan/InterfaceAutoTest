<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.practice.model">
    <!--  获取登录接口case  -->
    <select id="loginCase" parameterType="Integer" resultType="com.practice.model.LoginModel">
        select * from loginCase where id=#{id}
    </select>

    <!--  获取添加用户接口case  -->
    <select id="addUserCase" parameterType="Integer" resultType="com.practice.model.AddUserModel">
        select * from addUserCase where id=#{id}
    </select>

    <!--  获取用户信息接口case  -->
    <select id="getUserInfoCase" parameterType="Integer" resultType="com.practice.model.GetUserInfoModel">
        select * from getUserInfoCase where id=#{id}
    </select>

    <!--  获取用户列表接口case  -->
    <select id="getUserListCase" parameterType="Integer" resultType="com.practice.model.GetUserListModel">
        select * from getUserListCase where id=#{id}
    </select>

    <!--  更新/删除用户接口case  -->
    <select id="updateUserInfoCase" parameterType="Integer" resultType="com.practice.model.UpdateUserInfoModel">
        select * from updateUserInfoCase where id=#{id}
    </select>
</mapper>