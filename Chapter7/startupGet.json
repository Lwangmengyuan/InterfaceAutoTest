  {
    "description": "模拟一个没有参数的get请求",
    "request": {
      "uri": "/getDemo",
      "method": "get"
    },
    "response": {
      "text":"没有参数的get请求"
    }
  },
  {
    "description": "模拟一个带参数的get请求",
    "request": {
