[
  {
    "description": "这是一个带header信息的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "content-type":"application/json"
      },
      "json": {
        "name": "lily",
        "age": "20"
      }
    },
    "response": {
      "json": {
        "lily": "success",
        "status": "1"
      }
    }
  }
]